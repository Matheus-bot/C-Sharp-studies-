         Console.WriteLine(corFavorita);</div><div><br /></div><div>            var corFavorita = "vermelho";</div><div>            var modeloDoProduto = 9090;</div><div>            ====================================================*/</div><div>            /* dynamic corFavorita = "vermelho";</div><div>             Console.WriteLine(corFavorita);</div><div><br /></div><div>             corFavorita = 54756;</div><div>             Console.WriteLine(corFavorita);</div><div>             corFavorita = 98324.00f;</div><div>             Console.WriteLine(corFavorita);*/</div><div><br /></div><div>            Console.ReadLine();</div><div>        }</div><div>    }</div><div>}</div><hr /><div>const float PI = 3.14f;</div><div><br /></div><hr /><div>class Program</div><div>    {</div><div>        static void Main(string[] args)</div><div>        {</div><div>            string nome = "";</div><div><br /></div><div>            Console.WriteLine("Write your name please");</div><div>            /*string nome =  Console.ReadLine();*/</div><div>            nome = Console.ReadLine();</div><div><br /></div><div>            Console.WriteLine("Your name is ");</div><div>            Console.WriteLine(nome);</div><div><br /></div><div>            Console.ReadLine();</div><div>        }</div><div>    }</div><hr /><div>static void Main(string[] args)</div><div>        {</div><div>            int numeroQualquer = 29 + 20 - 90;</div><div>            int numeroMulti = 20 * 3;</div><div>            float numeroDiv = 5.0f/2;</div><div><br /></div><div>            int teste = (2 + 2) * 10;</div><div><br /></div><div>            Console.WriteLine(numeroQualquer);</div><div>            Console.WriteLine(numeroMulti);</div><div>            Console.WriteLine(numeroDiv);</div><div>            Console.WriteLine(teste);</div><div>            Console.ReadLine();</div><div>        }</div><hr /><div>int a = 10;</div><div>            int b = 20;</div><div>            int c = 2;</div><div>            if (a &lt; b)</div><div>            {</div><div>                Console.WriteLine("It is  true");</div><div>            } else if (a &gt; c ) </div><div>            {</div><div>                Console.WriteLine("Executated");</div><div>            }else</div><div>            {</div><div>                Console.WriteLine("It is false");</div><div>            }</div><div>            Console.ReadLine();</div><div>2:31:09</div><hr /><div>Console.WriteLine("Digite sua idade ");</div><div>            int idade = int.Parse(Console.ReadLine());</div><div><br /></div><div>            if (idade &gt;= 0 &amp;&amp; idade &lt;= 11)</div><div>            {</div><div>                Console.WriteLine("É uma criança");</div><div><br /></div><div>            } </div><div>            else if (idade &gt;= 12 &amp;&amp; idade &lt;= 18)</div><div>            {</div><div>                Console.WriteLine("Você é um adolescente!");</div><div>            } </div><div>            else if (idade &gt;= 19 &amp;&amp; idade &lt;= 60) </div><div>            {</div><div>                Console.WriteLine("Você é um adulto ");</div><div>            } </div><div>            else</div><div>            {</div><div>                Console.WriteLine("Vecê é idoso");</div><div>            }   </div><div>            Console.ReadLine();</div><hr /><div>class Program</div><div>    {</div><div>        static void Main(string[] args)</div><div>        {</div><div>            /*</div><div>            GerarPreco(60);</div><div>            GerarPreco(-40);</div><div>            GerarPreco(-74);*/</div><div><br /></div><div><br /></div><div>            int soma1 = Somar(1, 4, 8);</div><div>            int soma2 = Somar(2, 4, 62);</div><div>            int soma3 = Somar(23, 4, 5);</div><div><br /></div><div>            Console.WriteLine(soma1);</div><div>            Console.WriteLine(soma2);</div><div>            Console.WriteLine(soma3);</div><div><br /></div><div>            Console.ReadLine();</div><div><br /></div><div>        }</div><div>        // void = nada sem retorno</div><div>        static void ExibirMsg()</div><div>        {</div><div>            Console.WriteLine("This sistem is cool");</div><div>            Console.WriteLine("I am using functions");</div><div>            Console.WriteLine("Welcome to functions ");</div><div>        }</div><div><br /></div><div>        static void GerarPreco(int preco )</div><div>        {</div><div>            int precoAbs = Math.Abs(preco);</div><div>            int valorFinal = precoAbs + (2 * precoAbs);</div><div>            Console.WriteLine("Valor final é: " + valorFinal);</div><div><br /></div><div>        }</div><div><br /></div><div>        static int Somar(int a, int b, int c)</div><div>        {</div><div>            int resultadoFinal = a + b + c;</div><div>            return resultadoFinal;</div><div>        }</div><div><br /></div><div>    }</div><div><br /></div><div>3:36:00</div><hr /><div>class Program</div><div>    {</div><div>        static void Main(string[] args)</div><div>        {</div><div>            string[] produto = new string[5] {</div><div>                "Sea of thieves",//0</div><div>                "FIFA",          //1</div><div>                "Minecraft" ,    //2</div><div>                "Half-Life",     //3</div><div>                "Portal",        //4</div><div>            };</div><div><br /></div><div>            int[] valores = { 40, 50, 60, 70, 20 };</div><div><br /></div><div>            produto[1] = "FIFA 2021";</div><div>            Console.WriteLine(produto[1]) ;</div><div><br /></div><div>            Console.ReadLine();</div><div>        }</div><div>    }</div><hr /><div>class Program</div><div>    {</div><div>        static void Main(string[] args)</div><div>        {</div><div><br /></div><div>            string cor = "verde";</div><div><br /></div><div>            switch(cor) {</div><div>                case "Vermelho":</div><div>                    Console.WriteLine("Sua cor favorita é vemelho!");</div><div>                    break;</div><div>                case "Amarelo":</div><div>                    Console.WriteLine("Sua cor favorita é Amarelo!");</div><div>                    break;</div><div>                case "Azul":</div><div>                    Console.WriteLine("Sua cor favorita é Azul!");</div><div>                    break;</div><div>                case "Rosa":</div><div>                    Console.WriteLine("Sua cor favorita é Rosa!");</div><div>                    break;</div><div>                default:</div><div>                    Console.WriteLine("Sua cor favorita eu não sei!");</div><div>                    break;</div><div><br /></div><div>            }</div><div><br /></div><div>            Console.ReadLine();</div><div><br /></div><div>        }</div><div><br /></div><div><br /></div><hr /><div>class Program</div><div>    {</div><div><br /></div><div>        enum Cor { Azul, Verde, Amarelo, Vermelho }</div><div>   </div><div>        static void Main(string[] args)</div><div><br /></div><div><br /></div><div>        {</div><div><br /></div><div>            Cor  corFavorita = Cor.Vermelho;</div><div>            Cor corFavoritaDaCarla = Cor.Azul;</div><div><br /></div><div>            Console.WriteLine((Cor)2);</div><div>            Console.WriteLine(corFavoritaDaCarla);</div><div><br /></div><div>            Console.ReadLine();</div><div><br /></div><div>        }</div><div>    }</div><div><br /></div><hr /><div>class Program</div><div>    {</div><div><br /></div><div><br /></div><div>        enum Cor { Azul, Verde, Amarelo, Vermelho }</div><div><br /></div><div>        enum Opcao { Criar = 1, Deletar, Editar, Listar, Atualizar }</div><div>        </div><div>            </div><div>        static void Main(string[] args)</div><div><br /></div><div><br /></div><div>        {</div><div>            Console.WriteLine("Selecione uma das opções abaixo");</div><div>            Console.WriteLine("1-Criar\n2-Deletar\n3-Editar\n4-Listar\n5-Atualizar");</div><div><br /></div><div>            int index = int.Parse(Console.ReadLine());</div><div>            Opcao opcaoSelecionada = (Opcao)index;</div><div><br /></div><div>            switch (opcaoSelecionada)</div><div>            {</div><div>                case Opcao.Criar:</div><div>                    Console.WriteLine("Você quer criar algo!");</div><div>                    break;</div><div>                case Opcao.Deletar:</div><div>                    Console.WriteLine("DELETE! DELETE IMEDIATAMENTE!");</div><div>                    break;</div><div>                case Opcao.Editar:</div><div>                    Console.WriteLine("Editar é muito bom");</div><div>                    break;</div><div>                default:</div><div>                    Console.WriteLine("Opção não encontrada !");</div><div>                    break;</div><div>            }</div><div>            Console.ReadLine();</div><div>        }</div><div>    }</div><hr /><div>{</div><div><br /></div><div>            int contador = 0;</div><div><br /></div><div>            while (contador &lt; 10 )</div><div>            {</div><div>                Console.WriteLine(contador + 1);</div><div>                Console.WriteLine("Rodando o while");</div><div>              <b>// contador = contador + 1;</b></div><div><b>               // contador += 1;</b></div><div><b>                 contador ++;</b></div><div><br /></div><div>            }</div><div><br /></div><div>            Console.WriteLine("Fim da linha ");</div><div><br /></div><div>            Console.ReadLine();</div><div><br /></div><div>        }</div><div><br /></div><div><br /></div><hr /><div>int contador2 = 0;</div><div><br /></div><div>            do {</div><div><br /></div><div>                Console.WriteLine("Rodando ");</div><div>                contador2++;</div><div><br /></div><div>            } while ( contador2 &lt; 20);</div><hr /><div>string[] nomes = { "Amanda", "Beatriz", "Claudia", "Daniela", "Edmundo", "Fabricio" };</div><div><br /></div><div>            foreach (string nome in nomes) //<span style="--en-highlight:green;background-color: #b7f7d1;"> para cada palavra no array nomes, repita o bloco de códio </span></div><div>            {</div><div>                Console.WriteLine(nome);</div><div>            }</div><div><br /></div><div>            Console.ReadLine();</div><hr /><div>string[] nomes = { "Amanda", "Beatriz", "Claudia", "Daniela", "Edmundo", "Fabricio" };</div><div><br /></div><div>            for (int contador =  0; contador &lt; nomes.Length  ; contador++  )</div><div>            {</div><div>                Console.WriteLine(nomes[contador]);</div><div>                </div><div>            }</div><div>            Console.ReadLine();</div><div><br /></div><div>for (int contador =  1; contador &lt; nomes.Length  ; contador++  )</div><div>            {</div><div>                Console.Write(contador );</div><div>                Console.WriteLine(nomes[contador]);</div><div>            }</div><div><br /></div><div>string[] nomes = { "Amanda", "Beatriz", "Claudia", "Daniela", "Edmundo", "Fabricio" };</div><div><br /></div><div>            for (int contador =  1; contador &lt; nomes.Length  ; contador++  )</div><div>            {</div><div>                Console.Write(contador  );</div><div>                Console.WriteLine(nomes[contador]);</div><div>                </div><div>            }</div><div>            for (int contador2 = 5; contador2 &gt; -1; contador2--)</div><div>            {</div><div>                Console.WriteLine(contador2);</div><div>                Console.WriteLine(nomes[contador2]);</div><div>            }</div><div><br /></div><div>            Console.ReadLine();</div><hr /><div><br /></div><div><br /></div><div><br /></div></en-note>      ]]>
